<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.sziit.dao.StudentMapper">
    <!--根据主键查询-->
    <select id="selectStudentById" resultType="cn.edu.sziit.model.Student">
        select * from student where id = #{id}
    </select>
    <!--    查询所有学生-->
    <select id="selectStudents" resultType="cn.edu.sziit.model.Student">
        select * from student
    </select>
</mapper>